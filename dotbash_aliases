alias shrinkpdf='~/Dropbox/Developments/shell/shrinkpdf.sh'
alias weather='~/Dropbox/Developments/shell/weather.sh'

alias backupclassroomserver='~/Dropbox/Developments/shell/classroomserverbackup.sh'
alias itmo='~/Dropbox/Developments/c_devl/itmo/itmo'

alias create-project='~/Dropbox/Developments/shell/create-project.sh'
alias cdweb='cd ~/apps/web'
